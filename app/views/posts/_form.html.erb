<div class="view">
<%= form_with(model: @post, local: true) do |form|%>
<div class="form-group mt-5">
    <%= form.label :title%><br>
    <%= form.text_area :title, class: "form-control mt-1" %></>
<div class="form-group mt-4">
    <%= form.label :body %><br>
    <%= form.text_area :body, class: "form-control mt-1" %>    <br>
    <%= form.submit class: "btn btn-outline-primary mt-1" %>
<% end %> 